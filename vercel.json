{
  "builds": [
    {
      "src": "cancer/wsgi.py",
      "use": "@now/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "cancer/wsgi.py"
    }
  ],
  "functions": {
    "main": {
      "runtime": "python3.9"
    }
  }
}
